   "cell_type": "code",
   "execution_count": 10,
   "id": "b4d45f55",
   "metadata": {},
   "outputs": [],
   "source": [
    "cols_keep = [\"Element\",\"Value\",\"N\",\"P\",\"K\",\"temperature\",\"humidity\",\"ph\",\"rainfall\",\"crop\"]\n",
    "final_df = merged[cols_keep].copy()\n",
    "final_df = final_df[final_df[\"Element\"] == \"Production\"].copy()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 11,
   "id": "62aae39d",
   "metadata": {},
   "outputs": [],
   "source": [
    "numeric_cols = [\"Value\",\"N\",\"P\",\"K\",\"temperature\",\"humidity\",\"ph\",\"rainfall\"]\n",
    "for col in numeric_cols:\n",
    "    Q1 = final_df[col].quantile(0.25)\n",
    "    Q3 = final_df[col].quantile(0.75)\n",
    "    IQR = Q3 - Q1\n",
    "    low = Q1 - 1.5 * IQR\n",
    "    high = Q3 + 1.5 * IQR\n",
    "    final_df = final_df[(final_df[col] >= low) & (final_df[col] <= high)]"
   ]
  },
