   "cell_type": "code",
   "execution_count": 26,
   "id": "5470f4d5",
   "metadata": {},
   "outputs": [],
   "source": [
    "from sklearn.model_selection import GridSearchCV\n",
    "\n",
    "# base XGB model\n",
    "xg_base = XGBRegressor(\n",
    "    random_state=42,\n",
    "    n_jobs=-1,\n",
    ")\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 27,
   "id": "90d8c20e",
   "metadata": {},
   "outputs": [],
   "source": [
    "param_grid = {\n",
    "    \"max_depth\": [3, 5],\n",
    "    \"learning_rate\": [0.05, 0.1],\n",
    "    \"n_estimators\": [200, 300],\n",
    "    \"subsample\": [0.8],\n",
    "    \"colsample_bytree\": [0.8],\n",
    "}\n",
    "\n",
    "grid = GridSearchCV(\n",
    "    estimator=xg_base,\n",
